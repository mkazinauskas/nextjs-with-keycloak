# Keycloak issuer (realm) base URL, e.g. http://localhost:8080/realms/my-realm
OIDC_AUTHORITY=http://localhost:8080/realms/next-js-with-keycloak

# Public client id registered in the realm
OIDC_CLIENT_ID=nextjs-with-keycloak-app

# Optional client secret for confidential clients
OIDC_CLIENT_SECRET=

# Optional custom scopes, defaults to "openid profile email"
OIDC_SCOPE="openid profile email"

# Override redirect URLs if they differ from the defaults
OIDC_REDIRECT_URI=http://localhost:3000/api/oidc/callback
OIDC_POST_LOGOUT_REDIRECT_URI=http://localhost:3000/api/oidc/logout/callback
